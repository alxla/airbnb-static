---
title: Static AirBnB
---

    <!-- Your banner code -->
    <div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url(<%= image_path "background.jpg" %>);">
      <div class="banner-content">
        <h1>Some Stuff</h1>
        <p>Very cool stuff, useful and smart</p>
        <a class="btn btn-primary btn-lg">GET IT</a>
      </div>
    </div>
    <!-- Your features code -->
    <div class="features padded">
      <div class="container text-center small">
        <div class="row">
          <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
            <%= image_tag "globe.png" %>
            <h2>Discover the world</h2>
            <p>The earth is your new home.</p>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
            <%= image_tag "dollar.png" %>
            <h2>Pay online</h2>
            <p>No cash issues anymore.</p>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
           <%= image_tag "beer.png" %>
           <h2>Meet people</h2>
            <p>Discover new culture and lifestyle.</p>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
            <%= image_tag "heart.png" %>
            <h2>Share and love</h2>
            <p>Join a community you will love.</p>
          </div>
        </div>
      </div>
    </div>
    <!-- Your cards code -->
    <div class="padded wrapper-grey">
      <div class="container text-center">
        <div class="row">
          <div class="col-xs-12">
            <h2>Your next trip</h2>
          </div>
          <div class="col-xs-12">
            <div class="row">
              <div class="col-xs-12 col-sm-4">
                <!-- insert <div class="card"> -->
                <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-brussels.jpeg');">
                  <div class="card-category">Brussels</div>
                  <div class="card-description">
                    <h2>Anne's home</h2>
                    <p>Lovely house</p>
                  </div>
                  <img class="card-user avatar" src="images/anne.jpg">
                  <a class="card-link" href="#" ></a>
                </div>
              </div>
              <div class="col-xs-12 col-sm-4">
                <!-- insert <div class="card"> -->
                <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-london.jpeg');">
                  <div class="card-category">London</div>
                  <div class="card-description">
                    <h2>Romain's home</h2>
                    <p>Nice flat in Picadilly</p>
                  </div>
                  <img class="card-user avatar" src="images/romain.jpeg">
                  <a class="card-link" href="#" ></a>
                </div>
              </div>
              <div class="col-xs-12 col-sm-4">
                <!-- insert <div class="card"> -->
                <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-paris.jpg');">
                  <div class="card-category">Paris</div>
                    <div class="card-description">
                      <h2>Seb's home</h2>
                      <p>Charming room with patio</p>
                    </div>
                    <img class="card-user avatar" src="images/seb.jpg">
                    <a class="card-link" href="#" ></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Your newsletter code -->
    <div class="newsletter padded">
      <div class="container text-center">
        <div class="row">
          <div class="col-xs-4">
          </div>
          <div class="col-xs-4">
            <h2>Get news from us</h2>
            <p>Leave us your mail and get news from us</p>
            <form action="#">
              <div class="form-group form-inline">
                <input type="email" class="form-control" placeholder="bob@gmail.com">
                <input type="submit" value="Sign In" class="btn btn-primary">
              </div>
            </form>
          </div>
          <div class="col-xs-4">
          </div>
        </div>
      </div>
    </div>
    <!-- Your map code -->
    <div id="map" style="width: 100%; height: 600px;"></div>

    <!-- Including Bootstrap JS (with its jQuery dependency) so that dynamic components work -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>
    <script type="text/javascript">
    var myLatlng = new google.maps.LatLng(48.8655909, 2.378983);

    var myOptions = {
      zoom: 16,
      center: myLatlng,
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      styles: [] // TODO: replace [] by array from https://snazzymaps.com/
    };

    var map = new google.maps.Map(document.getElementById('map'), myOptions);
    var marker = new google.maps.Marker({
      position: myLatlng,
      map: map,
      title: "You are here!"
    });
    </script>
